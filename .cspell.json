  "version": "0.2",
  "language": "en",
  "allowCompoundWords": true,
  "caseSensitive": false,
  "ignoreRandomStrings": true,
  "words": [
    "bivariant",
    "contravariance",
    "gcanti",
    "NAND",
    "noshiro",
    "rgba",
